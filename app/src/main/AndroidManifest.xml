<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.restaurant.teditiansukma">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Restaurant Tedi"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name="com.restaurant.teditiansukma.RestaurantActivity"></activity>
        <activity android:name="com.restaurant.teditiansukma.ProfileActivity" />
        <activity android:name="com.restaurant.teditiansukma.auth.ChangePasswordActivity" />
        <activity android:name="com.restaurant.teditiansukma.auth.RegisterActivity" />
        <activity android:name="com.restaurant.teditiansukma.auth.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.restaurant.teditiansukma.DashboardActivity" />
    </application>

</manifest>